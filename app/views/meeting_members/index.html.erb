<p id="notice"><%= notice %></p>

<h1>Meeting Members</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Meeting</th>
      <th>Organizer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_members.each do |meeting_member| %>
      <tr>
        <td><%= meeting_member.member_id %></td>
        <td><%= meeting_member.meeting_id %></td>
        <td><%= meeting_member.organizer %></td>
        <td><%= link_to 'Show', meeting_member %></td>
        <td><%= link_to 'Edit', edit_meeting_member_path(meeting_member) %></td>
        <td><%= link_to 'Destroy', meeting_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting Member', new_meeting_member_path %>
