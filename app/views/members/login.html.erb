<p id="notice"><%= notice %></p>

<h1>Login</h1>

<br>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.owner %></td>
        <td><%= link_to 'login', member %></td>
      </tr>
    <% end %>
  </tbody>
</table>
